{
  "name": "uslugi-watcher",
  "version": "1.0.0",
  "description": "Monitor uslugi.io news API for new items",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node index.js -u https://dg.uslugi.io/lv/api/news -f id_news -b '{\"reception\":\"garden\"}'",
    "start:example": "node index.js -u http://localhost:3000/api/news -f id -b '{\"category\":\"test\"}'",
    "dev": "node --watch index.js -u https://dg.uslugi.io/lv/api/news -f id_news -b '{\"reception\":\"garden\"}'",
    "serve:example": "node example/server.js"
  },
  "dependencies": {
    "commander": "^14.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "node-fetch": "^3.3.2"
  },
  "keywords": [
    "monitor",
    "news",
    "api",
    "webhook",
    "ifttt"
  ],
  "author": "",
  "license": "MIT"
}
